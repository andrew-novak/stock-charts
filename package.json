{
  "name": "electron-stock-charts",
  "version": "1.0.0",
  "description": "Desktop app for visualizing stock charts.",
  "author": {
    "name": "Andrew Novak",
    "email": "contact@andrewnovak.co.uk"
  },
  "private": "true",
  "scripts": {
    "bundle": "rm -rf ./prod/bundle && concurrently \"npm run bundle:main\" \"npm run bundle:renderer\"",
    "bundle:main": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.main.prod.babel.js",
    "bundle:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.renderer.prod.babel.js",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "release:only": "rm -rf ./release && electron-builder build --config ./config/builder.config.js",
    "release": "npm run bundle && npm run release:only",
    "start": "node -r @babel/register ./scripts/checkPortInUse.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r @babel/register ./src/main/index.js",
    "start:bundle": "electron ./bundle/main.prod.js",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.config.renderer.dev.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrew-novak/electron-stock-charts.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrew-novak/electron-stock-charts/issues"
  },
  "homepage": "https://github.com/andrew-novak/electron-stock-charts#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "concurrently": "^6.0.0",
    "core-js": "^3.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "detect-port": "^1.3.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.2.1",
    "prompts": "^2.4.0",
    "react-refresh": "^0.9.0",
    "redux-devtools-extension": "^2.13.9",
    "style-loader": "^2.0.0",
    "tmp": "^0.2.1",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@canvasjs/react-stockcharts": "^1.0.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^1.4.0",
    "electron-store": "^7.0.2",
    "plotly.js": "^2.23.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-simple-code-editor": "^0.11.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "scramjet": "^4.36.9",
    "url-loader": "^4.1.1"
  }
}
